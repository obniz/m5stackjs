{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            false
        ],
        "interface-name": [false],
        "no-bitwise": [false],
        "variable-name": false,
        "object-literal-sort-keys" : false,
        "max-classes-per-file" : false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/webpack-replace/*.js"
        ]
    }
}